package com.learn.ittrationst;

public class loops {

	public static void main(String[] args) {

		int a= 10;
		  while(a>=0) {
		  
		  System.out.println(--a);
		 
 }
		// efefwetqwetqwtw

		while (true) {
			System.out.println("in loop");
		}

		int b = 0;
		do {
			System.out.println(b);
			System.out.println("in looping do");
			b++;
		} while (b < 2);
	}

}
